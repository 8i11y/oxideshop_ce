<phpunit backupGlobals="true"
         backupStaticAttributes="false"
         bootstrap="bootstrap.php"
         cacheTokens="true"
         colors="false"
         convertErrorsToExceptions="true"
         convertNoticesToExceptions="true"
         convertWarningsToExceptions="true"
         forceCoversAnnotation="false"
         mapTestClassNameToCoveredClassName="false"
         printerClass="\OxidEsales\TestingLibrary\Printer"
         processIsolation="false"
         stopOnError="false"
         stopOnFailure="false"
         stopOnIncomplete="false"
         stopOnSkipped="false"
         beStrictAboutTestsThatDoNotTestAnything="false"
         verbose="false">
    <filter>
        <whitelist addUncoveredFilesFromWhitelist="true"
                   processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">../source/Application</directory>
            <directory suffix=".php">../source/Core</directory>
            <directory suffix=".php">../source/Internal</directory>
            <directory suffix=".php">../source/bin</directory>
            <directory suffix=".php">../source/migration</directory>
            <directory suffix=".php">../source/modules</directory>
            <directory suffix=".php">../source/Setup/Controller</directory>
            <directory suffix=".php">../source/Setup/Exception</directory>
            <file>../source/Setup/Core.php</file>
            <file>../source/Setup/Database.php</file>
            <file>../source/Setup/Language.php</file>
            <file>../source/Setup/Session.php</file>
            <file>../source/Setup/Setup.php</file>
            <file>../source/Setup/Utilities.php</file>
            <file>../source/Setup/View.php</file>
            <exclude>
                <directory suffix=".php">../source/Application/views</directory>
                <directory suffix=".php">../source/Application/translations</directory>
                <directory suffix=".php">../source/Core/Smarty</directory>
                <file>../source/Core/Autoload/UnifiedNameSpaceClassMap.php</file>
                <file>../source/Setup/De/lang.php</file>
                <file>../source/Setup/En/lang.php</file>
                <file>../source/Application/Controller/Admin/ShopCountries.php</file>
                <file>../source/tests/Integration/Encoding/UtfTest.php</file>
                <file>../source/tests/Integration/Core/Module/ModuleSmartyPluginDirectoriesTest.php</file>
                <file>../source/tests/Acceptance/FrontendTestCase.php</file>

                <directory suffix=".php">../source/tests/Acceptance/Admin</directory>
                <directory suffix=".php">../source/tests/Acceptance/Frontend</directory>
                <directory suffix=".php">../source/tests/Acceptance/International</directory>
                <directory suffix=".php">../source/tests/Acceptance/Javascript</directory>
                <directory suffix=".php">../source/tests/</directory>

                <file>../source/tests/Acceptance/AdminTestCase.php</file>
                <file>../source/tests/Acceptance/FrontendTestCase.php</file>
                <file>../source/tests/Unit/Core/EmailAzureTplTest.php</file>
                <file>../source/tests/Unit/Core/EmailTest.php</file>
                <file>../source/tests/Unit/Core/EmailUtf8Test.php</file>
                <file>../source/tests/Unit/Core/UtilsTest.php</file>
                <file>../source/tests/Unit/Core/UtilsViewTest.php</file>
                <file>../source/tests/Unit/Application/Controller/ContentTest.php</file>
                <file>../source/tests/Unit/Application/Controller/ShopControlTest.php</file>
                <file>../source/tests/Unit/Application/Model/ActionsTest.php</file>
                <file>../source/tests/Unit/Application/Model/CategoryTest.php</file>
                <file>../source/tests/Unit/Application/Model/NewsTest.php</file>
                <file>../source/tests/Unit/Application/Model/NewsletterTest.php</file>
                <file>../source/tests/Unit/Application/Component/Widget/CategoryTreeTest.php</file>
            </exclude>
        </whitelist>
    </filter>







</phpunit>